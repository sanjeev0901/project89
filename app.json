{
  "expo": {
    "name": "Spectagram",
    "slug": "Spectagram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#9df2e1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.testuser.Spectagram",
      "supportsTablet": true
    },
    "android": {
      "package": "com.testuser.Spectagram",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#9df2e1"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
